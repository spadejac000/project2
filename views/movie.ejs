<div class="container text-center movie content">
  <% movies.forEach(function(movies) { %>
    <% if (movies.Poster !== 'N/A' && movies.Runtime !== 'N/A') { %>
    <a href="info/<%= movies.imdbID %>">
      <h1><%= movies.Title %></h1>
      <img src="<%= movies.Poster %>">
    </a>
    <% } %>
  <% }); %>
</div>

