<% include partials/alerts %>

<h2 class="text-center">Movies</h2>


<div class="container">
  <div class="well grey darken-3">
    <form action="/movie" method="post">
      <label class="search">Search Movies:</label>
      <input class="search-input" type="text" name="search">
      <button class="search-btn waves-effect waves-light btn" type="submit" name="button">Search</button>
    </form>
  </div>
  <div class='row'>
    <% for(i=0; i< 9; i++){ %>
      <div class="col s4">
        <img src="<%= movies[i].Poster %>" class="poster">
        <h6 class="title"><%= movies[i].Title %></h6>
      </div>
    <% } %>
  </div>
</div>

