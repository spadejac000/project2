<h2 class="text-center">Movies</h2>


<div class="container">
  <div class="well">
    <form action="/movie" method="post">
      <label>Search Movies:</label>
      <input type="text" name="search">
      <button type="submit" name="button">Search</button>
    </form>
  </div>
  <div class='row'>
    <% for(i=0; i< 9; i++){ %>
      <div class="col s4">
        <img src="<%= movies[i].Poster %>" class="poster">
        <h6 class="title"><%= movies[i].Title %></h6>
      </div>
    <% } %>
  </div>
</div>

